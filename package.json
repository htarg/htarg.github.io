{
  "name": "blog-starter",
  "homepage": "https://htarg.github.io/",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "create:nojekyll": "touch out/.nojekyll",
    "create:cname": "echo blog.htarg.com >> out/CNAME",
    "create:others": "npm run create:nojekyll && npm run create:cname",
    "push:ghpages": "gh-pages -t -d out -b master",
    "deploy": "npm run build && npm run export && npm run create:others && npm run push:ghpages"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "date-fns": "2.10.0",
    "gray-matter": "4.0.2",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "remark": "11.0.2",
    "remark-html": "13.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "dotenv": "8.2.0",
    "gh-pages": "^2.2.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.2.0"
  }
}
